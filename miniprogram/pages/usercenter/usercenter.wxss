/* pages/usercenter/usercenter.wxss */


.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  margin: 12rpx;
  border-radius: 50%;
}

.userinfo-vector {
  margin: 12rpx;
}

.userinfo-nickname {
  color: rgb(83, 112, 136);
  font-size: 32rpx;
  margin: 4rpx;
}

.userinfo-percent {
  color: rgb(83, 112, 136);
  font-size: 24rpx;
  margin: 8rpx;
}